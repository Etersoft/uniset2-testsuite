<?xml version="1.0" encoding="utf-8"?>
<TestScenario>
	<TestList logfile="RESFILE" logfile_trunc="1">
		<test name="link replace test" num="1">
			<action set="MyName1=0" />
			<check test="MyName1=0"/>
			<action set="MyName1=1" />
			<check test="MyName1!=0"/>
		</test>
		<test name="link replace test 2" num="2">
			<action set="MyName2=0" />
			<check test="MyName2=0"/>
			<action set="MyName2=1" />
			<check test="MyName2!=0"/>
		</test>
		<test name="Global replace" num="3">
			<action set="MyGlobalVar=1" />
			<check test="MyGlobalVar!=0"/>
			<action set="MyGlobalVar=0" />
			<check test="MyGlobalVar=0"/>
		</test>
		<test name="Test replace" num="4">
			<action set="MyTestVar=0" />
			<check test="MyTestVar=0"/>
		</test>
	</TestList>
</TestScenario>
