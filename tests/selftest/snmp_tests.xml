<?xml version="1.0" encoding="utf-8"?>
<TestScenario type="snmp">
	<Config>
        <aliases>
            <item default="1" type="snmp" alias="snmp1" snmp="conf/snmp.xml"/>
            <item type="snmp" alias="snmp2" snmp="conf/snmp.xml"/>
        </aliases>
	</Config>
    <TestList>
        <test num="1" name="SNMP read tests" comm="Чтение по snmp" ignore="1">
			<check test="sysServ@node3>1" comment="serv"/>
			<check test="uptime@node1>1" comment="Uptime"/>
			<check test="uptimeName@node2>=1" comment="Статус батареи" config="snmp2"/>
        </test>
        <test num="2" name="SNMP write tests" comm="Запись по snmp">
			<action set="sysName@node3=10" comment="save sysName"/>
			<action set="sysServ2@node3=10" comment="save sysServ"/>
        </test>
    </TestList>
</TestScenario>
